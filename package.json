{
  "name": "cfSnippets-Site",
  "version": "0.0.2",
  "description": "A simple site for CFML example snippets and cheat sheets. See the [content repo](https://github.com/LearnCF/cfSnippets).",
  "main": "",
  "scripts": {
    "build:css": "postcss resources/assets/css/tailwind.css -o dist/css/app.css",
    "build:js": "parcel build resources/assets/js/app.js --out-dir resources/dist/js/",
    "watch:css": "watch 'npm run build:css' ./views ./layouts",
    "watch:js": "parcel watch resources/assets/js/app.js --out-dir resources/dist/js/"
  },
  "author": "Michael Born <mike@learncf.dev>",
  "homepage": "https://github.com/LearnCF/cfSnippets-site",
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.0.4"
  },
  "dependencies": {
    "autoprefixer": "^9.7.4",
    "cssnano": "^4.1.10",
    "highlight.js": "^9.18.0",
    "postcss-cli": "^7.1.0",
    "tailwindcss": "^1.1.4",
    "watch": "^1.0.2"
  }
}